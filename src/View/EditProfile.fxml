<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="pane" prefHeight="703.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EditProfile">
    <children>
        <TextField fx:id="emailF" layoutX="489.0" layoutY="204.0" text="email">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="lastNameF" layoutX="489.0" layoutY="140.0" text="lastname">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="firstNameF" layoutX="489.0" layoutY="86.0" text="firstname">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <Button fx:id="edit" layoutX="68.0" layoutY="375.0" mnemonicParsing="false" onAction="#handleEdit" text="Edit">
            <font>
                <Font name="Times New Roman Italic" size="18.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="942.0" layoutY="626.0" mnemonicParsing="false" onAction="#handleBack" text="Go Back">
            <font>
                <Font name="Times New Roman Italic" size="18.0" />
            </font>
        </Button>
        <ImageView fx:id="imageView" fitHeight="248.0" fitWidth="277.0" layoutX="83.0" layoutY="46.0" pickOnBounds="true" preserveRatio="true" />
        <TextField fx:id="address" layoutX="489.0" layoutY="258.0" text="address">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="phone" layoutX="489.0" layoutY="320.0" text="phone">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
    </children>
    <stylesheets>
        <URL value="@design.css" />
    </stylesheets>
</AnchorPane>
