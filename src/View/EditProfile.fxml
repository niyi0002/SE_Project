<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EditProfile">
    <children>
        <TextField fx:id="emailF" layoutX="355.0" layoutY="188.0" text="email">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="lastNameF" layoutX="355.0" layoutY="133.0" text="lastname">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="firstNameF" layoutX="355.0" layoutY="83.0" text="firstname">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <Button fx:id="edit" layoutX="108.0" layoutY="226.0" mnemonicParsing="false" onAction="#handleEdit" text="Edit">
            <font>
                <Font name="Times New Roman Italic" size="18.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="92.0" layoutY="288.0" mnemonicParsing="false" onAction="#handleBack" text="Go Back">
            <font>
                <Font name="Times New Roman Italic" size="18.0" />
            </font>
        </Button>
        <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="28.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true" />
        <TextField fx:id="address" layoutX="355.0" layoutY="243.0" text="address">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
        <TextField fx:id="phone" layoutX="355.0" layoutY="291.0" text="phone">
            <font>
                <Font name="Times New Roman Italic" size="12.0" />
            </font></TextField>
    </children>
</AnchorPane>
