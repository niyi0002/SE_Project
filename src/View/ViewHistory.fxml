<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="pane" prefHeight="703.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewHistory">
    <children>
        <TableView fx:id="history" layoutX="407.0" layoutY="107.0" prefHeight="224.0" prefWidth="486.0">
            <columns>
                <TableColumn fx:id="attenedEvents" prefWidth="243.0" text="Events that attened" />
                <TableColumn fx:id="date" prefWidth="242.0" text="Date" />
            </columns>
        </TableView>
        <TableView fx:id="donationTable" layoutX="406.0" layoutY="395.0" prefHeight="224.0" prefWidth="486.0">
            <columns>
                <TableColumn fx:id="donateAmount" prefWidth="243.0" text="Donation" />
                <TableColumn fx:id="donatedDate" prefWidth="242.0" text="Date" />
            </columns>
        </TableView>
        <Label fx:id="label1" layoutX="526.0" layoutY="60.0" prefHeight="14.0" prefWidth="257.0" text="Events that volunteer has attented">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Label>
        <Label fx:id="label2" layoutX="499.0" layoutY="361.0" text="Donations that volunteer has contributed">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Label>
        <Button fx:id="goBack" layoutX="1089.0" layoutY="635.0" mnemonicParsing="false" onAction="#handleGoBack" text="Go Back">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
    </children>
    <stylesheets>
        <URL value="@design.css" />
    </stylesheets>
</AnchorPane>
